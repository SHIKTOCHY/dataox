<https://data.ox.ac.uk/id/itservices/service-lifecycle-status/in-development>
  a <https://data.ox.ac.uk/id/itservices/ServiceLifecycleStatus>, skos:Concept ;
  skos:prefLabel "in development"@en ;
  skos:definition "A service not yet ready for production use." ;
  skos:inScheme <https://data.ox.ac.uk/id/itservices/service-lifecycle-status> .

<https://data.ox.ac.uk/id/itservices/service-lifecycle-status/production>
  a <https://data.ox.ac.uk/id/itservices/ServiceLifecycleStatus>, skos:Concept ;
  skos:prefLabel "in production"@en ;
