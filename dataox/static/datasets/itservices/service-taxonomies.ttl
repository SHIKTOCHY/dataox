<https://data.ox.ac.uk/id/itservices/service-type> a skos:ConceptScheme ;
  skos:prefLabel "service types"@en ;
  skos:note "A code list for IT Service service types." ;
  dcterms:publisher <http://oxpoints.oucs.ox.ac.uk/id/31337175> ;
  skos:topConcept
    <https://data.ox.ac.uk/id/itservices/service-type/user-facing> ,
